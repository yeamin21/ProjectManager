<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="621.0" prefWidth="657.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="projectMan.AddTask">
    <children>
        <Label contentDisplay="RIGHT" layoutX="252.0" layoutY="20.0" prefHeight="25.0" prefWidth="89.0"
               text="Add some tasks"/>
        <GridPane fx:id="gridpane_tasks" layoutX="210.0" layoutY="64.0" prefHeight="222.0" prefWidth="348.0">
            <columnConstraints>
                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="28.0" minHeight="-Infinity" prefHeight="28.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="28.0" minHeight="-Infinity" prefHeight="28.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TextField fx:id="txt_1" alignment="CENTER" prefHeight="33.0" prefWidth="175.0"
                           GridPane.halignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <Label prefHeight="25.0" prefWidth="47.0" text="Task: 1"/>
                <Button fx:id="btn_addMoreTask" mnemonicParsing="false" onMouseClicked="#setBtn_addMoreTask"
                        prefHeight="25.0" prefWidth="58.0" text="Add +1" GridPane.halignment="RIGHT"/>
            </children>
        </GridPane>
        <Button fx:id="btn_done" layoutX="353.0" layoutY="371.0" mnemonicParsing="false" onMouseClicked="#setBtn_done"
                prefHeight="29.0" prefWidth="72.0" text="Done"/>
    </children>
</AnchorPane>
